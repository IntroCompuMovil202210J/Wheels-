<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/initialFragment">

    <fragment
        android:id="@+id/initialFragment"
        android:name="com.example.wheelsplus.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_initialFragment2_to_groupFragment"
            app:destination="@id/groupFragment" />
        <action
            android:id="@+id/action_initialFragment2_to_mapFragment"
            app:destination="@id/initLocationFragment" />
    </fragment>
    <fragment
        android:id="@+id/groupFragment"
        android:name="com.example.navtest.GroupFragment"
        android:label="fragment_group"
        tools:layout="@layout/fragment_group" >
        <action
            android:id="@+id/action_groupFragment_to_initialFragment2"
            app:destination="@id/initialFragment" />
    </fragment>
    <fragment
        android:id="@+id/initLocationFragment"
        android:name="com.example.wheelsplus.InitLocationFragment"
        android:label="fragment_init_location"
        tools:layout="@layout/fragment_init_location" />
</navigation>